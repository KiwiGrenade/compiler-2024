PROGRAM IS IN
	IF 2 > 1 THEN
		WRITE 1;
	ELSE
		WRITE 0;
	ENDIF
	
	
	IF 2 >= 1 THEN
		WRITE 1;
	ELSE
		WRITE 0;
	ENDIF
	
	
	IF 1 >= 1 THEN
		WRITE 1;
	ELSE
		WRITE 0;
	ENDIF
	
	
	IF 1 < 2 THEN
		WRITE 1;
	ELSE
		WRITE 0;
	ENDIF
	
	
	IF 1 <= 2 THEN
		WRITE 1;
	ELSE
		WRITE 0;
	ENDIF
	

	IF 1 <= 1 THEN
		WRITE 1;
	ELSE
		WRITE 0;
	ENDIF
	

	IF 1 = 1 THEN
		WRITE 1;
	ELSE
		WRITE 0;
	ENDIF
	

	IF 2 != 1 THEN
		WRITE 1;
	ELSE
		WRITE 0;
	ENDIF
END
